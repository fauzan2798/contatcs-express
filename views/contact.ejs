<div class="container">
    <div class="row">
        <div class="col-md-8">
            <h2>Daftar Contact</h2>

            <table class="table">
                <thead>
                  <tr>
                    <th scope="col">#</th>
                    <th scope="col">First</th>
                    <th scope="col">Last</th>
                    <th scope="col">Handle</th>
                  </tr>
                </thead>
                <tbody>
                    <% contacts.forEach((contact, i) => { %>
                        <tr>
                            <th scope="row"><%= ++i %></th>
                            <td><%= contact.nama %></td>
                            <td><%= contact.nohp %></td>
                            <td>
                                <a href="" class="btn btn-success badge rounded-pill"><i class="bi bi-info-circle"></i> detail</a>
                            </td>
                          </tr>
                    <% }) %>
                </tbody>
              </table>
        </div>
    </div>
</div>